.PAGE 'HELPF'.OPT NOLIST;**********************************=$0801        ;START OF BASIC;*********************************;; BASIC FORMAT PROGRAM;; 10 SYS(2063);;*********************************.WORD BASIC    ;LINE LINK POINTER.BYTE $0A,$00  ;LINE NUMBER (10).BYTE 158      ;SYS COMMAND TOKEN.BYTE '(2063)' ;CALL TO $080F.BYTE $00      ;END OF LINEBASIC.BYTE $00,$00  ;END OF BASIC;**********************************=$080F        ;LOADER64 ORGIN;*********************************\        LDA #$0A ; SIX CHARACTERSHELPF   LDX #<FILNAM        LDY #>FILNAM        JSR $FFBD      ; call SETNAM        LDA #$02       ; filenumber 2        LDX $BA        BNE SKIP        LDX #$08       ; default to device number 8SKIP    LDY #$00       ; secondary address 0 (required for dir reading!)        JSR $FFBA      ; call SETLFS        JSR $FFC0      ; call OPEN (open the directory)        BCS ERROR      ; quit if OPEN failed        ; CHECK DRIVE ERROR CHANNEL        ; FILE NOT FOUND error etc.        LDX #$02       ; filenumber 2        JSR $FFC6      ; call CHKINLOOP    JSR $FFB7 ;READST        BNE EOF        JSR $FFCF ;CHRIN        JSR $FFD2     ; call CHROUT (print character)        JMP LOOP      ; continue until end of lineEOF     AND #$40        BEQ ERROR        LDA #$02 ; FILE 2        JSR $FFC3 ; CLOSE        JSR $FFCC ; CLRCHNERROR   NOP        ; ACCumulator contains BASIC error code        ; most likely error:        ; A = $05 (DEVICE NOT PRESENT)EXIT    LDA #$02       ; filenumber 2        JSR $FFC3      ; call CLOSE        JSR $FFCC     ; call CLRCHN        RTSFILNAM .BYTE 'EDHELP,S,R'.END